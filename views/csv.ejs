<%- include('components/header.ejs') %>

    <main id="auth">
        <div class="container">
            <div class="flex-row stretch">
                <div class="row span-1-of-2 visual flex-wrap">
                    <img src="/img/contact.svg" alt="">
                </div>
                <div class="row span-1-of-2 flex-wrap">
                    <form action="/user/csv-upload" method="POST" enctype="multipart/form-data">
                         <h2>CSV Upload</h2>
                        <div class="form-item">
                            <label for="name">
                                Select File
                            </label>
                            <input type="file" name="file" id="file" required>
                        </div>
                        <div class="form-item">
                            <button type="submit" class="btn cta">Send</button>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </main>

    <%- include('components/footer.ejs') %>